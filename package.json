{
  "name": "ng-nest-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:zh": "ng serve --configuration=zh",
    "build": "ng build --prod && npm run build:en && npm run build:zh",
    "build:en": "ng build --prod --configuration=production-en",
    "build:zh": "ng build --prod --configuration=production-zh",
    "build:ng-moon": "ng build ng-moon",
    "build:docs": "node ./scripts/build/generate-docs-ui-components.js",
    "test": "ng test ng-moon",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path=locale"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "highlight.js": "^9.15.6",
    "marked": "^0.6.2",
    "ngx-highlightjs": "^3.0.3",
    "remark": "^10.0.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "yaml-front-matter": "^4.0.0",
    "zone.js": "~0.8.26"
