{
  "name": "ng-nest-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:zh": "ng serve --configuration=zh",
    "build": "ng build --prod && npm run build:en && npm run build:zh",
    "build:en": "ng build --prod --configuration=production-en",
    "build:zh": "ng build --prod --configuration=production-zh",
    "build:ng-moon": "ng build ng-moon",
    "build:old-docs": "node ./scripts/build/generate-docs-ui-components.js",
    "build:docs": "npm run build:scripts && node ./scripts/dist-ts/generate/docs",
    "build:scripts": "tsc -p scripts",
    "test": "ng test ng-moon",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --output-path=locale"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.0.0-rc.4",
    "@angular/cdk": "~8.0.0-rc.1",
    "@angular/common": "~8.0.0-rc.4",
    "@angular/compiler": "~8.0.0-rc.4",
    "@angular/core": "~8.0.0-rc.4",
    "@angular/forms": "~8.0.0-rc.4",
    "@angular/platform-browser": "~8.0.0-rc.4",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.4",
    "@angular/router": "~8.0.0-rc.4",
    "core-js": "^2.6.5",
    "highlight.js": "^9.15.6",
    "marked": "^0.6.2",
    "ngx-highlightjs": "^3.0.3",
    "remark": "^10.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "yaml-front-matter": "^4.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.4",
    "@angular-devkit/build-ng-packagr": "~0.800.0-rc.4",
    "@angular/cli": "~8.0.0-rc.4",
    "@angular/compiler-cli": "~8.0.0-rc.4",
    "@angular/language-service": "~8.0.0-rc.4",
    "@types/fs-extra": "^5.0.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/marked": "^0.6.5",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
