{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "module": "esnext",
    "paths": {
      "@ng-nest/ui": ["dist/ng-nest/ui"],
      "@ng-nest/ui/*": ["dist/ng-nest/ui/*"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.ts"],
  "exclude": ["src/test.ts", "src/**/*.spec.ts", "src/environments/environment.prod.ts"],
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
